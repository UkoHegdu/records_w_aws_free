            "Action": [
                "s3:*",
                "lambda:*",
                "apigateway:*",
                "cloudfront:*",
                "dynamodb:*",
                "ses:*",
                "sns:*",
                "sqs:*",
                "ssm:*",
                "cloudwatch:*",
                "events:*",
                "iam:*",
                "logs:*",
                "states:*"
